<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.scz.globallogic">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".GlobalLogicApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_pokeball"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_pokeball"
        android:supportsRtl="true"
        android:theme="@style/Theme.Globallogic">
        <activity
            android:name=".presentation.dashboard.ACDashboard"
            android:exported="true"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>